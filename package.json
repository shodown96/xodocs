{
    "name": "xodocs",
    "version": "1.0.0",
    "description": "Generate OpenAPI docs from Zod schemas and controller comments in Express.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "dev": "tsc -w",
        "test": "vitest",
        "prepare": "npm run build",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "release:dry": "semantic-release --dry-run"
    },
    "homepage": "https://github.com/shodown96/xodocs",
    "repository": {
        "type": "git",
        "url": "https://github.com/shodown96/xodocs"
    },
    "keywords": [
        "express",
        "zod",
        "swagger",
        "openapi",
        "typescript",
        "documentation",
        "developer-tools"
    ],
    "files": [
        "dist"
    ],
    "author": "Elijah Soladoye",
    "license": "MIT",
    "dependencies": {
        "express": "^5.1.0",
        "reflect-metadata": "^0.2.2",
        "swagger-ui-express": "^5.0.1",
        "ts-morph": "^26.0.0",
        "ts-node": "^10.9.2",
        "zod": "^3.21.4",
        "zod-openapi": "^4.2.4"
    },
    "devDependencies": {
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^13.0.1",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/npm": "^12.0.2",
        "@semantic-release/release-notes-generator": "^14.0.3",
        "@types/express": "^5.0.3",
        "@types/supertest": "^6.0.3",
        "@types/swagger-ui-express": "^4.1.8",
        "conventional-changelog-conventionalcommits": "^9.0.0",
        "prettier": "^3.6.2",
        "semantic-release": "^24.2.6",
        "supertest": "^7.1.1",
        "typescript": "^5.2.2",
        "vitest": "^3.2.4"
    },
    "peerDependencies": {
        "zod": "^3.21.4",
        "zod-openapi": "^4.2.4"
    }
}
